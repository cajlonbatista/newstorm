  },
  "build": {
    "appId": "com.cajlonbatista.vercel.app.newstorm",
    "productName": "Newstorm Panel",
    "copyright": "Copyright Â© 2021 ${author}",
    "mac": {
      "category": "public.dashboard.in"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
