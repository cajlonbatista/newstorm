      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-inline-react-svg": "^1.1.2",
    "concurrently": "^5.3.0",
    "electron": "^11.2.2",
    "electron-builder": "^22.9.1",
    "typescript": "^4.1.3",
    "wait-on": "^5.2.1"
